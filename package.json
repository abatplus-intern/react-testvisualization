{
    "name": "abatplus-react-boilerplate",
    "version": "0.0.1",
    "description": "React frontend template",
    "main": "src/index.tsx",
    "engines": {
        "node": ">=14.17.0",
        "npm": ">=8.1.1"
    },
    "browserslist": [
        "defaults"
    ],
    "scripts": {
        "start": "webpack serve --config webpack.config.js --env env=development",
        "build": "webpack --config webpack.config.js --env env=production",
        "test": "jest --config='./jest.config.js'",
        "test:watch": "jest --watchAll",
        "test:coverage": "jest --collect-coverage",
        "cypress": "npx cypress open",
        "test:ui": "npx cypress run --browser chrome",
        "lint": "eslint --ext js,jsx,ts,tsx .",
        "lint:fix": "npm run lint:fix:eslint",
        "lint:fix:eslint": "eslint --fix --ext js,jsx,ts,tsx .",
        "clean": "rimraf dist && rimraf coverage",
        "bundle:analyze": "rimraf webpack_stats.json && webpack --profile --json > webpack_stats.json && webpack-bundle-analyzer webpack_stats.json",
        "artifactory:login": "npm login --registry https://artifacts.i.mercedes-benz.com/artifactory/api/npm/switch-main-npm-releases/ --registry https://artifacts.i.mercedes-benz.com/artifactory/api/npm/plusng-main-npm-local/",
        "api:generate": "cd api && npx ts-node ./generator.ts"
    },
    "author": "abat+ GmbH for Mercedes-Benz AG",
    "license": "abat+ GmbH for Mercedes-Benz AG",
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mercedes-benz/mbui-comps": "^1.0.2",
        "@mercedes-benz/mui5-theme": "^2.0.4",
        "@mercedes-benz/typeface-mb-corpo-a": "^1.0.0",
        "@mercedes-benz/typeface-mb-corpo-s": "^1.0.0",
        "@mui/icons-material": "^5.10.16",
        "@mui/material": "^5.10.16",
        "@mui/styles": "^5.10.16",
        "@plusng/react-common-components": "^1.0.18",
        "file-saver": "^2.0.5",
        "notistack": "2.0.8",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-intl": "6.2.2",
        "react-router-dom": "6.4.3",
        "tss-react": "^4.4.4"
    },
    "devDependencies": {
        "@babel/cli": "7.19.3",
        "@babel/core": "7.20.5",
        "@babel/plugin-transform-runtime": "7.19.6",
        "@babel/preset-env": "7.20.2",
        "@babel/preset-react": "7.18.6",
        "@babel/preset-typescript": "7.18.6",
        "@babel/runtime": "7.20.6",
        "@cypress/react": "7.0.1",
        "@cypress/webpack-dev-server": "3.1.0",
        "@openapitools/openapi-generator-cli": "2.5.2",
        "@svgr/webpack": "6.5.1",
        "@types/chalk": "2.2.0",
        "@types/enzyme": "3.10.12",
        "@types/fs-extra": "9.0.13",
        "@types/jest": "29.2.3",
        "@types/node": "18.11.15",
        "@types/react": "18.0.25",
        "@types/react-dom": "18.0.9",
        "@typescript-eslint/eslint-plugin": "5.45.0",
        "@typescript-eslint/parser": "5.45.0",
        "@wojtekmaj/enzyme-adapter-react-17": "0.8.0",
        "babel-jest": "29.3.1",
        "babel-loader": "9.1.0",
        "browserslist": "4.21.4",
        "chalk": "^5.1.2",
        "css-loader": "6.7.2",
        "cypress": "11.2.0",
        "editorconfig": "1.0.1",
        "enzyme": "3.11.0",
        "eslint": "8.28.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.31.11",
        "eslint-plugin-react-hooks": "4.6.0",
        "fork-ts-checker-webpack-plugin": "7.2.13",
        "html-webpack-plugin": "5.5.0",
        "jest": "29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "mini-css-extract-plugin": "2.7.1",
        "prettier": "2.8.0",
        "rimraf": "3.0.2",
        "style-loader": "3.3.1",
        "ts-jest": "29.0.3",
        "ts-node": "10.9.1",
        "typescript": "4.9.3",
        "webpack": "5.75.0",
        "webpack-bundle-analyzer": "4.7.0",
        "webpack-cli": "5.0.0",
        "webpack-dev-server": "4.11.1"
    },
    "overrides": {
        "cheerio": "1.0.0-rc.10"
    }
}
